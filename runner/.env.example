# Supabase connection (use service role key, NOT anon key)
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_SERVICE_ROLE_KEY=...

# Runner configuration
RUNNER_ID=runner-001
POLL_INTERVAL_MS=5000

# Runner mode: 'fake' for testing, 'real' for actual Docker execution
RUNNER_MODE=real
# Safety guard: fake runners cannot claim queue jobs unless this is explicitly true
RUNNER_ALLOW_FAKE_CLAIM=false
# Heartbeat interval for runner status tracking
RUNNER_HEARTBEAT_INTERVAL_MS=10000

# Job execution configuration (for real mode)
JOBS_DIR=./jobs
DOCKER_IMAGE=cgra/cgra-flow:ui
DOCKER_TIMEOUT_MS=600000
